<div ng-controller="StudentInfoDetailsController">
    <div class="clearfix"></div>
    <div class="panel panel-default">
        <div class="panel-heading">Basic Details</div>
        <div class="panel-body">
            <div class="col-sm-6">
                <div class="col-md-6 col-sm-6">
                    <label class="control-label">Blood Goup </label>
                </div>
                <div class="col-md-6 col-sm-6">
                    <span >{{Student.bloodGroup}}</span>
                    
                </div>
            </div>
            <div class="col-sm-6">
                <div class="col-md-6 col-sm-6">
                    <label class="control-label">Birth Place </label>
                </div>
                <div class="col-md-6 col-sm-6">
                   <span>{{Student.birthPlace}}</span>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="col-md-6 col-sm-6">
                    <label class="control-label">Nationalty</label>
                </div>
                <div class="col-md-6 col-sm-6">
                    <input type="text" class="form-control" ng-model="Student.nationalty" ng-required="true" name="nationalty" />
                </div>
            </div>
            <div class="col-sm-6">
                <div class="col-md-6 col-sm-6">
                    <label class="control-label">Languages </label>
                </div>
                <div class="col-md-6 col-sm-6">
                    <input type="text" class="form-control" ng-model="Student.languages" ng-required="true" name="languages" />
                </div>
            </div>
            <div class="col-sm-6">
                <div class="col-md-6 col-sm-6">
                    <label class="control-label">Religion</label>
                </div>
                <div class="col-md-6 col-sm-6">
                    <input type="text" class="form-control" ng-model="Student.religion" ng-required="true" name="religion" />
                </div>
            </div>
        </div>

        <div class="panel-heading">Profile Settings</div>
        <div class="panel-body">
            <div class="col-sm-6">
                <div class="col-md-6 col-sm-6">
                    <label class="control-label">Is SMS Enabled </label>
                </div>
                <div class="col-md-6 col-sm-6">
                    <input type="checkbox" ng-model="Student.isSMSEnabled" name="isSMSEnabled" />
                </div>
            </div>
        </div>
    </div>
        <div class="clearfix"></div>
        <div class="col-sm-8 col-sm-offset-4" style="margin-top:20px;">

            <button style="width: 100px; height: 30px; margin-right:10px;" class="btn" id="Button5" type="button" ng-click="EditStudentInfo();">
                <span>Edit</span>
            </button>
        </div>
    </div>





